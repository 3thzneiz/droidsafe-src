<configuration>
  <appender name="FILE" class="ch.qos.logback.core.FileAppender">
    <file>droidsafe-gen/droidsafe.log</file>
    <append>false</append>
    <encoder>
      <pattern>%msg%n </pattern>
    </encoder>
  </appender>

  <logger name="droidsafe.analyses.CatchBlocks" level="INFO" />

  <root level="WARN">
    <appender-ref ref="FILE" />
  </root>
    
</configuration>

