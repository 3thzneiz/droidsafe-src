#!/bin/bash
SCRIPTNAME=`basename $0`
BINDIR=`dirname $0`
APPDIR=`(cd $BINDIR/..; pwd)`
LIBDIR=$BINDIR/lib
CONFIGDIR=$APPDIR/config
JAVA_RT=$JAVA_HOME/jre/lib/rt.jar
APPJAR=$APPDIR/bin/CallTraceViewer-int.jar
APPCLASS=droidsafe.gui.CallTraceViewer

for JARFILE in $LIBDIR/*.jar; do
    APP_CLASSPATH=$APP_CLASSPATH:$JARFILE
done

APP_CLASSPATH=$APP_CLASSPATH:$CONFIGDIR

echo java -cp $APP_CLASSPATH:$APPJAR $APPCLASS  
java -cp $APP_CLASSPATH:$APPJAR $APPCLASS  

