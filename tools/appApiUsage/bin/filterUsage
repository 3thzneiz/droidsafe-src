#!/usr/bin/perl
#  

$inName = shift @ARGV;
$outName = shift @ARGV;

#
# syntax: <infile> <outfile> 
#

if (defined($inName)) {
	open($inFile, "<$inName") || die("Cannot open $inName \n");
} else {
    $inFile = \*STDIN;
}

if (defined($outName)) {
	open($outFile, ">$outName") || die("Cannot open $outName \n");
} else {
    $outFile = \*STDOUT;
}
   

%hash = ();

$skip = 0;

while(<$inFile>) {
    if (/Output Report/) {
        $skip = 0;
    }
    if (/Direct API/) {
        $skip = 1;
    }

    print $outFile $_ if !$skip;
}

