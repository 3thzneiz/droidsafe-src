#!/bin/bash
SCRIPTNAME=`basename $0`
BINDIR=`dirname $0`
APPDIR=`(cd $BINDIR/..; pwd)`
LIBDIR=$APPDIR/../../lib
DROIDSAFE_BINDIR=$APPDIR/../../bin
APPJAR=$APPDIR/bin/appApiUsage.jar

for JARFILE in $LIBDIR/*.jar $DROIDSAFE_BINDIR/*jar; do
    APP_CLASSPATH=$APP_CLASSPATH:$JARFILE
done

APP_CLASSPATH=$APPJAR:$APP_CLASSPATH
java -cp $APP_CLASSPATH droidsafe.tools.ApiUsageListing $1 $2 $3 $4 $5 $6 $7 $8 $9
